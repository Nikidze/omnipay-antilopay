{
    "name": "nikidze/omnipay-antilopay",
    "type": "library",
    "description": "Antilopay gateway for Omnipay",
    "keywords": [
        "omnipay",
        "antilopay"
    ],
    "homepage": "https://github.com/Nikidze/omnipay-antilopay",
    "license": "MIT",
    "authors": [
        {
            "name": "receiver",
            "email": "receiver8@yandex.ru"
        },
        {
            "name": "Nikidze",
            "email": "nikidze.dev@ya.ru"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-openssl": "*",
        "omnipay/common": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Antilopay\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}